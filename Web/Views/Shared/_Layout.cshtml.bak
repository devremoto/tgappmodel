@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
<!DOCTYPE html>
<html lang="en">
<!--[if IE 9]>
<html class="ie9" lang="en">    <![endif]-->
<!--[if IE 8]>
<html class="ie8" lang="en">    <![endif]-->
<head>
    <base href="/" />
    <title>TUGON</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Tugon Comunicação Visual e Informática">
    <meta name="author" content="Tugon Comunicação Visual e Informática">
    <!-- CORE CSS -->
    <link href="~/pages/assets/css/bootstrap.css" rel="stylesheet" type="text/css">
    <link href="~/pages/assets/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="~/pages/assets/css/elegant-icons.css" rel="stylesheet" type="text/css">
    <!-- SLIDER REVOLUTION CSS -->
    <link href="~/pages/assets/js/plugins/revolution/css/settings.css" rel="stylesheet" type="text/css">
    <link href="~/pages/assets/js/plugins/revolution/css/layers.css" rel="stylesheet" type="text/css">
    <link href="~/pages/assets/js/plugins/revolution/css/navigation.css" rel="stylesheet" type="text/css">
    <!-- THEME CSS -->
    <link href="~/pages/assets/css/main.css" rel="stylesheet" type="text/css">
    <link href="~/pages/assets/css/my-custom-styles.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="~/pages/assets/css/skins/gold.css" type="text/css">
    <!-- GOOGLE FONTS -->
    <link href='https://fonts.googleapis.com/css?family=Raleway:700,400,400italic,500' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Lato:400,400italic,700,300,300italic' rel='stylesheet' type='text/css'>
    <!-- FAVICONS -->
    <link rel="shortcut icon" href="~/pages/assets/ico/favicon.ico">
</head>
<body>
    @RenderBody()

    <!-- JAVASCRIPT -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/latest/TweenLite.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/latest/plugins/CSSPlugin.min.js"></script>
    <script src="~/pages/assets/js/jquery-2.1.1.min.js"></script>
    <script src="~/pages/assets/js/bootstrap.min.js"></script>
    <script src="~/pages/assets/js/plugins/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <script src="~/pages/assets/js/plugins/easing/jquery.easing.min.js"></script>
    <script src="~/pages/assets/js/plugins/revolution/js/jquery.themepunch.tools.min.js"></script>
    <script src="~/pages/assets/js/plugins/revolution/js/jquery.themepunch.revolution.min.js"></script>
    <script src="~/pages/assets/js/plugins/owl-carousel/owl.carousel.min.js"></script>
    <script src="~/pages/assets/js/plugins/countto/jquery.countTo.js"></script>
    <script src="~/pages/assets/js/plugins/jquery-waypoints/jquery.waypoints.min.js"></script>
    <script src="~/pages/assets/js/plugins/parsley-validation/parsley.min.js"></script>
    <script src="~/pages/assets/js/bravana.js"></script>

    <script src="~/lib/html-docx.js"></script>
    <script src="~/lib/FileSaver.js"></script>
    <script src="~/assets/js/jsrsasign.js"></script>
    <environment names="Development">
        <script src="~/dist/inline.js" asp-append-version="true"></script>
        <script src="~/dist/polyfills.js" asp-append-version="true"></script>
        <script src="~/dist/vendor.js" asp-append-version="true"></script>
        <script src="~/dist/main.js" asp-append-version="true"></script>
        <script src="~/dist/pages.js" asp-append-version="true"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="~/dist/inline.min.js.gz" asp-append-version="true"></script>
        <script src="~/dist/polyfills.min.js.gz" asp-append-version="true"></script>
        <script src="~/dist/vendor.min.js.gz" asp-append-version="true"></script>
        <script src="~/dist/main.min.js.gz" asp-append-version="true"></script>
        <script src="~/dist/pages.min.js.gz" asp-append-version="true"></script>
    </environment>

    @RenderSection("scripts", required: false)
</body>
</html>

